{
  "name": "rgutil",
  "version": "1.1.5",
  "description": "$R是基于ES6的JavaScript函数库",
  "main": "index.js",
  "utilName": "$R",
  "scripts": {
    "rbuild": "rollup -c",
    "db": "npm run dev:build",
    "pb": "npm run pro:build",
    "dev:build": "rimraf dist && cross-env NODE_ENV=development rollup -c",
    "pro:build": "rimraf dist && cross-env NODE_ENV=production rollup -c ",
    "watch": "rollup -c -w",
    "dev": "vite",
    "build": "vite build",
    "build:all": "vite build && npm run pro:build",
    "test": "jest --coverage"
  },
  "dependencies": {
    "prismjs": "^1.22.0",
    "vue": "^3.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@babel/register": "^7.4.4",
    "@babel/types": "^7.4.4",
    "@rollup/plugin-babel": "^5.0.4",
    "@vue/compiler-sfc": "^3.0.2",
    "babel-plugin-annotate-pure-calls": "^0.4.0",
    "babel-plugin-import-export-rename": "^1.0.1",
    "cross-env": "^7.0.2",
    "jest": "^26.6.3",
    "rimraf": "^3.0.2",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-uglify": "^6.0.4",
    "rollup-watch": "^4.3.1",
    "sass": "^1.30.0",
    "vite": "^1.0.0-rc.13"
  },
  "keywords": [
    "ES6函数库",
    "vue3",
    "vite"
  ],
  "author": "GuCangRan",
  "license": "MIT"
}
